;; Example .clojure-mcp/config.edn with custom model configurations

{;; Custom model configurations
 :models {:openai/my-fast-gpt {:model-name "gpt-4o"
                               :temperature 0.3
                               :max-tokens 2048}

          :openai/my-creative-gpt {:model-name "gpt-4o"
                                   :temperature 1.5
                                   :max-tokens 8192}

          :anthropic/my-claude {:model-name "claude-3-5-sonnet-20241022"
                                :temperature 0.7
                                :max-tokens 4096}

          :anthropic/my-reasoning-claude {:model-name "claude-3-5-sonnet-20241022"
                                          :max-tokens 8192
                                          :thinking {:enabled true
                                                     :return true
                                                     :send true
                                                     :budget-tokens 4096}}

          :google/my-gemini-flash {:model-name "gemini-2.5-flash"
                                   :temperature 0.5
                                   :max-tokens 4096
                                   :google {:allow-code-execution true}}}

 ;; Other configuration options
 :allowed-directories ["."]
 :cljfmt true
 :bash-over-nrepl true
 :scratch-pad-load false}
